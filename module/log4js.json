{
    "appenders": [{
        "type": "console",
        "category": "console"
    }, {
        "type": "dateFile",
        "filename": "./logs/",
        "pattern": "yyyyMMddhh.txt",
        "absolute": false,
        "alwaysIncludePattern": true,
        "category": "logInfo"
    }],
    "replaceConsole": true,
    "levels": {
        "logInfo": "debug"
    }
}
